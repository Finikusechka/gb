#5

profit = int(input('Введите свою прибыль > '))
cost = int(input('Введите расходы фирмы > '))
profitability = int(0) #Рентабельность
gain = int #Выручка
people = int #Количество сотрудников
profit_on_people = int
#print(profit, cost) Вывод
if profit > cost:
    print('Поздравляем, Вы в плюсе')
    gain = profit - cost
    profitability = profit / gain
    people = int(input('Сколько у Вас сотрудников ? '))
    profit_on_people = profit / people
    print('Ваша рентабельность :', profitability)
    print(f"Каждый Ваш сотрудник принес по: {profit_on_people:.0f} рублей(ю) прибыли")
else:
    print('Увы, надо работать лучше :(')

# Задание 6

first_distance = int(input('Со скольки км спортсмен начал ? >'))
final_distance = int(input('Сколько км спортсмен должен пробежать ? > '))
percent = int
coin = int(1)
while first_distance < final_distance:
    percent = first_distance / 100 * 10
    first_distance = first_distance + percent
    coin = coin + 1
else: print(coin)
